{
  "name": "doulevo",
  "version": "0.0.4",
  "description": "Prototype version of Doulevo. Don't use in production.",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "start": "ts-node src/index.ts create test-project --force --local-plugin=./test-plugin",
    "start:dev": "nodemon",
    "c": "npm run clean",
    "clean": "rm -rf build/*",
    "b": "npm run build",
    "build": "tsc --incremental",
    "bw": "npm run build:watch",
    "build:watch": "tsc  --incremental --watch",
    "prepublishOnly": "npm test && npm run clean && tsc",
    "t": "npm run test",
    "test": "jest --all",
    "tw": "npm run test:watch",
    "test:watch": "jest --watch --all",
    "build-win": "tsc && npx nexe build/index.js -t windows-x64-12.16.2",
    "build-mac": "npm run prepublishOnly && npx nexe build/index.js -t mac-x64-12.16.2",
    "build-linux": "npm run prepublishOnly && npx nexe build/index.js -t linux-x64-12.16.2"
  },
  "bin": {
    "doulevo": "./bin/cli"
  },
  "keywords": [],
  "author": "ashley@codecapers.com.au",
  "license": "MIT",
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/fs-extra": "^9.0.11",
    "@types/globby": "^9.1.0",
    "@types/handlebars": "^4.1.0",
    "@types/inquirer": "^7.3.1",
    "@types/jest": "^26.0.20",
    "@types/minimist": "^1.2.1",
    "@types/node": "^14.14.25",
    "@types/ora": "^3.2.0",
    "@types/uuid": "^8.3.0",
    "globby": "^11.0.3",
    "jest": "^26.6.3",
    "nexe": "^4.0.0-beta.18",
    "nodemon": "^2.0.7",
    "source-map-support": "0.5.19",
    "ts-jest": "^26.5.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@codecapers/fusion": "^1.0.7",
    "chalk": "^4.1.1",
    "fs-extra": "^9.1.0",
    "handlebars": "^4.7.7",
    "inflate-template": "^1.1.9",
    "inquirer": "^8.0.0",
    "minimist": "^1.2.5",
    "ora": "^5.4.0",
    "uuid": "^8.3.2"
  }
}
